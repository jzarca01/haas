{
  "name": "houmous",
  "version": "0.0.1",
  "description": "Houmous as a service CLI",
  "author": "Jeremie Zarca @jzarca01",
  "bin": {
    "houmous": "node ./bin/run.cjs"
  },
  "type": "module",
  "homepage": "https://github.com/jzarca01/houmous",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jzarca01/houmous.git"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@inquirer/confirm": "^1.0.7",
    "@oclif/core": "^2",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.4.7",
    "axios": "^1.4.0",
    "axios-retry": "^3.5.0",
    "inquirer": "^9.2.6",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "libphonenumber-js": "^1.10.30"
  },
  "devDependencies": {
    "@googlemaps/google-maps-services-js": "^3.3.30",
    "@oclif/test": "^2.3.19",
    "@types/inquirer": "^9.0.3",
    "@types/inquirer-autocomplete-prompt": "^3.0.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.18.29",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.3",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5"
  },
  "oclif": {
    "bin": "houmous",
    "dirname": "houmous",
    "commands": "./dist/commands",
    "topicSeparator": ":",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "npm run lint",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md",
    "start": "npm run build && node ./bin/run.cjs"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": {
    "url": "https://github.com/jzarca01/houmous/issues"
  },
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
